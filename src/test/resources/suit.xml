<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="LoginAdmin" verbose="1" >

    <listeners>
        <listener class-name="HangTester.nopCommerce.Listenner.TestListenner"></listener>
    </listeners>

    <test name="Login"  >
        <parameter name="browser" value="chrome"></parameter>
        <classes>
            <class name="HangTester.nopCommerce.Test.LoginAdminTest" >
                <methods>
                    <include name="LoginValid"/>
                    <include name="LoginInvalidPW"/>
                </methods>
            </class>
        </classes>
    </test>

</suite>
